{
    "compilerOptions": {
        "target": "ESNext",
        "module": "ESNext",
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "declaration": true,
        "moduleResolution": "node",
        "jsx": "preserve",
        "resolveJsonModule": true,
        "newLine": "lf",
        "strict": true,
        "jsxImportSource": "solid-js",
        "types": [
            "vite/client",
            "vite-plugin-pwa/client"
        ],
        "noEmit": true,
        "isolatedModules": true
    }
}
